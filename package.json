{
  "name": "native-circular-status",
  "version": "1.0.0",
  "description": "Progress bar modeled on the animation from Apple Music and App Store on iOS",
  "author": "Jakub Biesiada",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prepack": "npm run prettier && npm run lint && npm run build",
    "clean": "rimraf lib/*",
    "build": "tsc",
    "prebuild": "npm run clean",
    "test": "jest --coverage",
    "watch": "npm run build -- --watch",
    "watch:test": "npm run test -- --watch",
    "lint": "eslint --fix 'src/**/*.tsx'",
    "prettier": "prettier --write 'src/**/*.tsx'",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/native-ly/native-circular-status.git"
  },
  "keywords": [
    "react",
    "android",
    "ios",
    "library",
    "mobile",
    "react-native",
    "component",
    "download",
    "animation",
    "progress-bar",
    "loading",
    "play",
    "pause",
    "indicator",
    "expo"
  ],
  "bugs": {
    "url": "https://github.com/native-ly/native-circular-status/issues"
  },
  "homepage": "https://github.com/native-ly/native-circular-status#readme",
  "dependencies": {
    "native-icons": "1.3.0",
    "react-native-progress": "^5.0.0",
    "react-native-svg": "^12.1.1"
  },
  "devDependencies": {
    "@testing-library/jest-native": "^4.0.4",
    "@testing-library/react-native": "^8.0.0",
    "@tsconfig/react-native": "^1.0.4",
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.34",
    "@types/react-native": "^0.66.2",
    "@typescript-eslint/parser": "^5.3.0",
    "babel-jest": "^27.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "cz-emoji": "^1.3.1",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react-native": "^3.11.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1",
    "react": "^17.0.2",
    "react-native": "^0.66.2",
    "react-test-renderer": "^17.0.2",
    "semantic-release": "^18.0.0",
    "semantic-release-gitmoji": "^1.4.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-native": ">=0.59.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-emoji"
    }
  },
  "lint-staged": {
    "src/**/*.tsx": [
      "npm run prettier",
      "npm run lint"
    ]
  }
}
